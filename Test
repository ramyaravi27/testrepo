  <label class="switchs" style="cursor: not-allowed;" >
  <input type="checkbox"  (click)="errpop('imagegallery')"  [(ngModel)]="imagegalerystatuss" >
  <div class="sliders rounds" style="cursor: not-allowed;" >
    <span class="offs"></span>
    <span class="ons"></span>
  </div>
 
</label>
                            </div>
                          </div>
