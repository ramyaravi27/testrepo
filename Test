  <div class="alertPopup" *ngIf="alertShown">
    <img class="imageView" src="./assets/logon/alert.png" alt="">
    <div class="fontStyleAlert"> {{alertMessage}}</div>
    <button class="greenbtn  btn-primary" (click)="alertClose()"> OK </button>
  </div>
</div>
