onKey1(event: any) {
  var lowerCaseLetters = /[a-z]/g;
  var upperCaseLetters = /[A-Z]/g;
  var numbers = /[0-9]/g;
  var special = /[#$^+=!*()@%&]/g;
  if (event.target.value.match(lowerCaseLetters)) {
    this.passkey = true
  }
  else {
    this.passkey = false
  }
  if (event.target.value.match(upperCaseLetters)) {
    this.passkey1 = true
  }
  else {
    this.passkey1 = false
  }
  if (event.target.value.match(numbers)) {
    this.passkey2 = true
  }
  else {
    this.passkey2 = false
  }
  if (event.target.value.length >= 8) {
    this.passkey3 = true
  }
  else {
    this.passkey3 = false
  }
  if (event.target.value.match(special)) {
    this.passkey4 = true
  }
  else {
    this.passkey4 = false
  }


}
