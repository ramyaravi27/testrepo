                <video  style="width: 100%;height:100%;" controlsList="nodownload nofullscreen noremoteplayback">   
                  <source [src]="api_url +item.file" accept="video/*">  
                  <track src="" kind="subtitles" srclang="en" label="English"> 
                </video>
